{
  "name": "ide-haskell-hoogle",
  "main": "./lib/ide-haskell-hoogle",
  "version": "0.1.1",
  "description": "Show hoogle information for symbol under cursor",
  "keywords": [
    "ide-haskell",
    "ide",
    "haskell",
    "help",
    "hoogle"
  ],
  "repository": "https://github.com/atom-haskell/ide-haskell-hoogle",
  "license": "MIT",
  "engines": {
    "atom": ">=1.13.0 <2.0.0"
  },
  "activationCommands": {
    "atom-text-editor": ["ide-haskell-hoogle:show-doc-for-symbol", "ide-haskell-hoogle:show-web-doc-for-symbol"]
  },
  "dependencies": {
    "atom-highlight": "^0.3.0",
    "atom-select-list": "^0.2.0",
    "atom-space-pen-views": "^2.0.3",
    "cheerio": "^1.0.0-rc.2",
    "core-decorators": "^0.19.0",
    "etch": "^0.12.5",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.2",
    "@types/core-decorators": "^0.10.32",
    "@types/electron": "^1.6.10",
    "@types/node": "^8.0.14",
    "@types/request-promise-native": "^1.0.6",
    "atom-haskell-tslint-rules": "0.0.6",
    "tslint": "^5.6.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.0"
  },
  "deserializers": {
    "HoogleDocView": "createDocView",
    "HoogleWebView": "createWebView"
  }
}
